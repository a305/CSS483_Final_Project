package com.uwb.bt2j.util.strings;

public class SStringExpandable<T> {
	
	public SStringExpandable(int s, int m, int i) {
		cs_ = null;
		printcs_ = null;
		len_ = 0;
		sz_ = 0;
		if(i > 0) {
			expandNoCopy(i);
		}
	}
	
	public SStringExpandable(int s, int m, int i, double sz) {
		cs_ = null;
		printcs_ = null;
		len_ = 0;
		sz_ = 0;
		if(i > 0) {
			expandNoCopy(sz);
		}
	}
}
