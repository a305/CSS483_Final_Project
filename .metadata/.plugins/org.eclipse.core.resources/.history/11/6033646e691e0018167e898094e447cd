package com.uwb.bt2j.util.types;

public class RedBlack <K, P>{
	public RedBlack(int pageSz, int cat) {
		//perPage_ = pageSz/
		pages_ = cat;
		clear();
	}
	
	public RedBlackNode lookup(K key) {
		RedBlackNode cur = root_;
		while(cur != NULL) {
			if((*cur) == key) return cur;
			if((*cur) < key) {
				cur = cur->right;
			} else {
				cur = cur->left;
			}
		}
		return NULL;
	}
}
