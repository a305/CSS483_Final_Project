  }
  
  public int getBase() {
  
  }
  
  public int getStretchNaive() {
    
  }
  
  public int getStretch() {
    
  }
  
  public final double numRefs() {
	  return nrefs_;
  }
  
  public final double approxLen() {
	  return refLens_[elt];
  }
  
  public final Boolean loaded() {
	  return loaded_;
  }
  
  public final double pastedOffset() {
	  return refOffs_[idx];
  }
  
