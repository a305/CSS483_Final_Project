package com.uwb.bt2j.util.strings;

public class SString<T> {
	public SString() {
		
	}
	
	public SString(double sz) {
		resize(sz);
	}
	
	public SString(SString<T> o) {
		install(o.cs_, o.len_);
	}
	
	public SString(T b, double sz) {
		install(b, sz);
	}
	
	public SString(T b) {
		install(b, b.toString().length());
	}
	
	
}
