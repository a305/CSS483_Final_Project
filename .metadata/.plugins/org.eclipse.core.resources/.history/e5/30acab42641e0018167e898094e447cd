package com.uwb.bt2j.util.types;

public class EMap <T>{
	private int cat_;
	private EMap<T> list_;
	private int sz_;
	private int cur_;
	
	public EMap(int cat) {
		cat_ = cat;
		list_ = null;
		sz_ = 128;
		cur_ = 0;
	}
	
	public EMap(int isz, int cat) {
		cat_ = cat;
		list_ = null;
		sz_ = isz;
		cur_ = 0;
	}
	
	public EMap(EMap<T> o) {
		list_ = null;
	}
	
	public int size() {
		return cur_;
	}
	
	public boolean empty() {
		return cur_ == 0;
	}
	
}
