package com.uwb.bt2j.util.types;

public class EHeap<T> {
	public void insert(T o) {
		int pos = l_.size();
		l_.push_back(o);
		while(pos > 0) {
			int parent = (pos-1) >> 1;
			if(l_[pos] < l_[parent]) {
				T tmp = new T(l_[pos]);
				l_[pos] = l_[parent];
				l_[parent] = tmp;
				pos = parent;
			} else break;
		}
	}
}
