package com.uwb.bt2j.aligner.dp;

import java.io.OutputStream;

public class DPRect {
	
    public long refl;
    public long refr;
  
    public long refl_pretrim;
    public long refr_pretrim;
  
    public double triml;
    public double trimr;
  
    public double corel;
    public double corer;
    public double maxgap;
  
    public void write(OutputStream os) {
    	os.write( refl + ',' + refr + ',' + refl_pretrim + ','
		   + refr_pretrim + ',' + triml + ',' + trimr + ','
		   + corel + ',' + corer + ',' + maxgap);
    }
  
    public Boolean entirelyTrimmed() {
    
    }
    
    public void initIVal() {
      
    }
	public DPRect(int cat) {
		refl = refr = triml = trimr = corel = corer = 0;
	}
}
