package com.uwb.bt2j.aligner.seed;

public class EEHit {
	public EEHit() {
		reset();
	}
	
	public void reset() {
		top = bot = 0;
		fw = false;
		e1.reset();
		e2.reset();
		score = Long.MIN_VALUE;
	}
	
	public void init(
			long top_,
			long bot_,
			Edit e1_,
			Edit e2_,
			boolean fw_,
			long score_) {
		
	}
}
