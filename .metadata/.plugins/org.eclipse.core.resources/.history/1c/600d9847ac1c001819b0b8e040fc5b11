package com.uwb.bt2j.aligner;

public class Interval {
	protected Coord upstream_;
	protected double len_;
	
	public Interval() {
		reset();
	}
	
	public Interval(Coord upstream, double len) {
		init(upstream, len);
	}
	
	public Interval(double rf, double of, Boolean fw, double len) {
		init(rf,of,fw,len);
	}
	
	public void init(Coord upstream, double len) {
		upstream_ = upstream;
		len_ = len;
	}
	
	public void init(double rf, double of, Boolean fw, double len) {
		upstream_.init(rf,of,fw);
		len_ = len;
	}
}
